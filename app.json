{
  "expo": {
    "name": "GymApp",
    "scheme": "com.supabase",
    "slug": "GymApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/l1.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/l1.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A2C"
    },
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/l1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.gymapp.gymleveling"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-secure-store"
    ]
  }
}
