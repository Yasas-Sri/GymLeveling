{
  "expo": {
    "name": "GymApp",
    "scheme": "GymApp",
    "slug": "GymApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/l1.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A2C"
    },
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
       "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
